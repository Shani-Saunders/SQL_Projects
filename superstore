SELECT price, item_id, item_name, category, stock_quantity, average_rating
FROM superstore
ORDER BY price ASC;

SELECT COUNT(stock_quantity), category
FROM superstore
GROUP BY category;


SELECT AVG(average_rating),category
FROM superstore
WHERE category = 'Kitchen Supplies';

SELECT MAX(price), item_name
FROM superstore
GROUP BY item_id
ORDER BY(PRICE) DESC;
